    {
      "id": 1,
      "title": "HUD Image Viewer",
      "type": "text",
      "description": "Displays the latest HUD snapshot in MinIO.",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "options": {
        "content": "<img src=\"http://minio:9000/evidence/evidence/${region}/hud-latest.svg\" width=\"100%\">",
        "mode": "html"
      }
    },
    {
      "id": 2,
      "title": "Clause Execution Timeline",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "{clause=\"hud.render\", tenant=\"$persona\"}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "title": "Severity Heatmap",
      "type": "heatmap",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({clause=\"news.ingest\", tenant=\"$persona\"} [$__interval]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "Worldmap",
      "type": "geomap",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "count by (region) (count_over_time({clause=\"news.ingest\", tenant=\"$persona\"} [$__interval]))",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "title": "Evidence Bundles",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "{clause=\"evidence.bundle\", tenant=\"$persona\"}",
          "refId": "A"
        }
      ]
    }
